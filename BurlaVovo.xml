<?xml version='1.0' encoding='UTF-8' ?>
<?charles serialisation-version='2.0' ?>
<rewriteSet-array>
  <rewriteSet>
    <active>true</active>
    <name>pular telefone</name>
    <hosts>
      <locationPatterns>
        <locationMatch>
          <location>
            <protocol>https</protocol>
            <host>login2.caixa.gov.br</host>
            <path>/auth/realms/internet/login-actions/registration</path>
          </location>
          <enabled>true</enabled>
        </locationMatch>
      </locationPatterns>
    </hosts>
    <rules>
      <rewriteRule>
        <active>true</active>
        <ruleType>7</ruleType>
        <matchValue>&lt;input inputmode=&quot;text&quot; placeholder=&quot;:&quot; id=&quot;fullName&quot; name=&quot;fullName&quot; type=&quot;text&quot; value=&quot;&quot; autocomplete=&quot;name&quot; required /&gt;</matchValue>
        <matchHeaderRegex>false</matchHeaderRegex>
        <matchValueRegex>false</matchValueRegex>
        <matchRequest>false</matchRequest>
        <matchResponse>true</matchResponse>
        <newValue>&lt;input inputmode=&quot;text&quot; placeholder=&quot;:&quot; id=&quot;fullName&quot; name=&quot;fullName&quot; type=&quot;text&quot; value=&quot;&quot; autocomplete=&quot;name&quot; required /&gt; &lt;input placeholder=&quot;Numero do celular&quot; class=&quot;input-action&quot; id=&quot;user.attributes.numeroTelefone&quot; name=&quot;user.attributes.numeroTelefone&quot; required /&gt; &lt;input placeholder=&quot;Confirma numero celular&quot; class=&quot;input-action&quot; id=&quot;numeroTelefoneConfirma&quot; name=&quot;numeroTelefoneConfirma&quot; required /&gt;</newValue>
        <newHeaderRegex>false</newHeaderRegex>
        <newValueRegex>false</newValueRegex>
        <matchWholeValue>false</matchWholeValue>
        <caseSensitive>false</caseSensitive>
        <replaceType>2</replaceType>
      </rewriteRule>
    </rules>
  </rewriteSet>
</rewriteSet-array>